<?xml version="1.0" encoding="UTF-8"?>
<dbwebdescriptor version='2.0'>
	<label>MR Node</label>
	<description>MR Node</description>
	<queries>
		<!-- METADATA -->
		<ws-query id='METADATA' url='http://mrnode.mil.intra/LRF/XMLWeb/ProcessDescriptor/descriptor/COMMON/MRN_METADATA2.xml' xpath='/dbqueries/*' auth='RealTimeNodeAuth'>
			<parameter name='pcINFO'>pINFO</parameter>
			<parameter name='pcLANG'>pLANG</parameter>
			<parameter name='pcRID'>pRID</parameter>
			<parameter name='pcCDN'>pUSER</parameter>
		</ws-query>
		<querystring id='ILIAS_IREP_VER' database='irep'>
			select LOOK.NLK_CODE, LOOK.NLK_DESC, LOOK.NLK_AVALUE as CHK FROM ILIAS.V$NLOOKUPS LOOK where LOOK.NLK_CODE='SYSVER'
		</querystring>
	</queries>	
	<parameters>
		<parameter name='pRID' value=''/>		
		<parameter name='pLANG' value='EN'/>		
		<parameter name='pINFO' value='DUSM'/>
		<parameter name='pMODE' value='Main'/>
		<!-- Only For Pillar Node Authentication
		<parameter name="pUSER" value='' overrulingvalue="$auth('sAMAccountName')" toUppercase='true'/>	
		-->		
	</parameters>
</dbwebdescriptor>